<launch>
  <param name="use_sim_time" value="true" />
  <param name="publisher/robot_description" textfile="$(find robot_state_publisher)/test/pr2.urdf" />

  <node pkg="rosbag" name="rosbag" type="play" args="--clock $(find robot_state_publisher)/test/joint_states_indexed.bag" />
  <node pkg="robot_state_publisher" name="publisher_state_pub" type="robot_state_publisher">
    <remap from="robot_description" to="publisher/robot_description" />
  </node>

  <test test-name="test_publisher" pkg="robot_state_publisher" type="test_publisher" />
</launch>
